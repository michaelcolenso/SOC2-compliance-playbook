  "quality_gates": {
    "1.1": {
      "type": "data_points",
      "file": "research/cost_analysis.json",
      "field": "cost_data_points",
      "min_count": 20
    },
    "1.2": {
      "type": "interviews",
      "min_count": 3
    },
    "1.3": {
      "type": "file_count",
      "files": [
        "compliance/control_matrix.json",
        "compliance/mvcs.md"
      ],
      "min_count": 2
    },
    "2.1": {
      "type": "file_count",
      "files": [
        "structure/playbook_outline.json",
        "structure/content_specs.md"
      ],
      "min_count": 2
    },
    "2.2": {
      "type": "file_count",
      "files": [
        "templates/designs/security_policies.md",
        "templates/designs/access_control.md",
        "templates/designs/vendor_management.md",
        "templates/designs/incident_response.md",
        "templates/designs/audit_prep.md"
      ],
      "min_count": 5
    },
    "3.1": {
      "type": "page_count",
      "chapters_dir": "chapters",
      "min_pages": 80
    },
    "3.2": {
      "type": "file_count",
      "files": [
        "templates/index.json"
      ],
      "min_count": 1
    },
    "3.3": {
      "type": "file_count",
      "files": [
        "tools/readiness_scorecard/index.html",
        "tools/policy_generator/index.html",
        "tools/auditor_matcher/index.html"
      ],
      "min_count": 3
    },
    "4.1": {
      "type": "file_count",
      "files": [
        "case_studies/case_study_01.md",
        "case_studies/case_study_02.md",
        "case_studies/case_study_03.md"
      ],
      "min_count": 3
    },
    "4.2": {
      "type": "file_count",
      "files": [
        "visuals/flowcharts/",
        "visuals/infographics/",
        "visuals/checklists/"
      ],
      "min_count": 3
    },
    "4.3": {
      "type": "custom",
      "check_function": "validate_technical_accuracy"
    },
    "4.4": {
      "type": "beta_feedback",
      "min_responses": 3,
      "min_rating": 4.0,
      "min_would_pay_percent": 0.8
    },
    "5.1": {
      "type": "file_count",
      "files": [
        "product/soc2_playbook_complete.pdf",
